<?xml version="1.0"?>
<PropertyList>
 <path>cabin.ac</path>



 <animation>
  <type>material</type>
  <object-name>ceiling</object-name>
   <object-name>seats</object-name>
   <object-name>wall</object-name>
   <condition>
                <greater-than>
                    <property>sim/time/sun-angle-rad</property>
                    <value>1.55</value>
                </greater-than>
        </condition>
  <emission>
    <red>0.9</red>
    <green>0.75</green>
    <blue>0.6</blue>
    </emission>
 </animation>

</PropertyList>